using System;

class Program
{
    static void Main()
    {
        
        Console.Write("Ingrese el primer número (de dos dígitos): ");
        int num1 = int.Parse(Console.ReadLine());

        Console.Write("Ingrese el segundo número (de dos dígitos): ");
        int num2 = int.Parse(Console.ReadLine());

        
        int suma = num1 + num2;

        
        if (suma % 2 == 0)
        {
            Console.WriteLine("La suma de los dos números es par.");
        }
        else
        {
            Console.WriteLine("La suma de los dos números es impar.");
        }
    }
}
